@model IEnumerable<Silicus.UtilityContainer.Models.DataObjects.Utility>

@{
    ViewBag.Title = "Dashboard";
    var isRoleAvailable = ViewBag.noRoleForCUrrentUser;
}


<div class="row white-bg header-row M-T5">
    <div class="col-lg-12 col-md-12 col-sm-12">
        <h1 class="header-name">Dashboard</h1>
    </div>
</div>
<div class="row">
    <div class="box M-T5" style="border-top:5px solid #acb3b7;">
        <div class="f1">
            <div class="row">
                <div class="col-lg-6 col-md-6 col-sm-6">
                    <div class="el-element-overlay m-b-40">
                        <div class="white-box">
                            <div class="el-card-item">
                                <div class="el-card-avatar el-overlay-1">
                                    
                                    <img style="height:100px;width:90%;margin: 90px 30px 70px 30px;" src="~/Images/encourageLogo.png" />
                                    <div class="el-overlay">
                                        <ul class="el-info">
                                            <li>
                                                <a class="btn default btn-outline image-popup-vertical-fit" href="#" title="View">
                                                    <i class="fa fa-eye fa-2x" aria-hidden="true"></i>
                                                </a>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                                <div class="el-card-content">
                                    <h3 class="box-title">Utility for Award Nomination and Winner Declaration.</h3>

                                 
                                    <br>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-6 col-md-6 col-sm-6">
                    <div class="el-element-overlay m-b-40">
                        <div class="white-box">
                            <div class="el-card-item">
                                <div class="el-card-avatar el-overlay-1">
                                    
                                    <img style="height: 80px;width:75%;margin: 95px 30px 80px 65px;"  src="~/Images/enhanceLogo.png" />
                                    <div class="el-overlay">
                                        <ul class="el-info">
                                            <li>
                                                <a class="btn default btn-outline image-popup-vertical-fit" href="#" title="View">
                                                    <i class="fa fa-eye fa-2x" aria-hidden="true"></i>
                                                </a>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                                <div class="el-card-content">
                                    <h3 class="box-title">Enhance is collection of all development component.</h3>

                                   
                                    <br>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-6 col-md-6 col-sm-6">
                    <div class="el-element-overlay m-b-40">
                        <div class="white-box">
                            <div class="el-card-item" style="padding-bottom:0px;">
                                <div class="el-card-avatar el-overlay-1">
                                    
                                    <img style="height: 85px;width:65%;margin: 85px 30px 70px 90px;"  src="~/Images/ensureLogo.png" />
                                    <div class="el-overlay">
                                        <ul class="el-info">
                                            <li>
                                                <a class="btn default btn-outline image-popup-vertical-fit" href="#" title="View">
                                                    <i class="fa fa-eye fa-2x" aria-hidden="true"></i>
                                                </a>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                                <div class="el-card-content">
                                    <h3 class="box-title">An online portal for pre-employment assessments.</h3>

                                   
                                    <br>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-6 col-md-6 col-sm-6">
                    <div class="el-element-overlay m-b-40">
                        <div class="white-box">
                            <div class="el-card-item" style="padding-bottom:0px;">
                                <div class="el-card-avatar el-overlay-1">

                                    <img style="height: 83px;width:70%;margin: 88px 30px 70px 80px;"  src="~/Images/entimeLogo.png" />
                                    <div class="el-overlay">
                                        <ul class="el-info">
                                            <li>
                                                <a class="btn default btn-outline image-popup-vertical-fit" href="#" title="View">
                                                    <i class="fa fa-eye fa-2x" aria-hidden="true"></i>
                                                </a>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                                <div class="el-card-content">
                                    <h3 class="box-title">Tool of Tracking Employee time and work.</h3>

                                  
                                    <br>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
