@using Kendo.Mvc.UI;
@{
    ViewBag.Title = "Dashboard";
    var user = Html.Partial("/Views/ManageUser/_GetUserListPartialView.cshtml");
}
<div class="main-content">
    <div class="fluid mt20">
        @if (TempData["Success"] != null && !string.IsNullOrWhiteSpace(Convert.ToString(TempData["Success"])))
        {
            <div class="alert alert-success" id="messageDiv">
                <strong>Success!</strong> <span id="messageContent">@Convert.ToString(TempData["Success"])</span>
            </div>
        }
        <div class="kendogrid">
            @(user)
        </div>
    </div>
</div>



<script id="confirmationTemplate" type="text/x-kendo-template">
    <div class="popupMessage"></div>
    </br>
    <hr />
    <div class="dialog_buttons">
        <input type="button" class="confirm_yes k-button" value="Yes" style="width: 70px" />
        &nbsp;
        <input type="button" class="confirm_no k-button" value="No" style="width: 70px" />
    </div>
</script>
<script>

    $(document).ready(function () {
        $('#messageDiv').fadeOut(5000);
    });
</script>