@model Silicus.Ensure.Models.DataObjects.Panel

<div class="M-B20">

    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
    <div class="">
        @Html.LabelFor(model => model.PanelName, htmlAttributes: new { @class = "col-md-3" })
        <div class="col-md-9">
            @Html.EditorFor(model => model.PanelName, new { htmlAttributes = new { @class = "form-control" } })
            @Html.ValidationMessageFor(model => model.PanelName, "", new { @class = "text-danger" })
        </div>
    </div>

</div>


@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
