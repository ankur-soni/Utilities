@using Kendo.Mvc.UI;
@{
    ViewBag.Title = "Index";
}

@{
    ViewBag.Title = "Candidate Dashboard";
    var tagList = Html.Partial("~/Views/Tag/List.cshtml");
    var positionList = Html.Partial("~/Views/Positions/Positions.cshtml");
    var panleList = Html.Partial("~/Views/Panel/Panel.cshtml");
    var technologies = Html.Partial("~/Views/Technology/Technologies.cshtml");
}
<style>
    .k-loading-image {
        background-image: none !important;
    }
</style>
<!-- BEGIN PAGE TITLE-->
<div class="row">
    <div class="col-sm-12">
        <h1 class="page-title">
            Setup
        </h1>
    </div>
</div>

<div class="row">
    <div class="col-sm-12">
        <div class="M-B60">
            <div class="main-content">
                <div class="kendogrid">
                    @(Html.Kendo().TabStrip()
                .Name("Tabs")
                .Items(tabstrip =>
                {
                    tabstrip.Add().Text("Tags").Selected(true).Content(tagList.ToString());
                    tabstrip.Add().Text("Positions").Selected(false).Content(positionList.ToString());
                    tabstrip.Add().Text("Panels").Selected(false).Content(panleList.ToString());
                    tabstrip.Add().Text("Technologies").Selected(false).Content(technologies.ToString());
                })
                    )

                </div>
            </div>
        </div>
    </div>
</div>

<script>
    $(document).ready(function () {
        SelectedMenu('#setup-link');
        $("#messageDiv").fadeOut();
        SetNavigationMenuActive('Setup');
    });
</script>
