@model Silicus.Ensure.Web.Models.Test.TestSummaryViewModel
@using Kendo.Mvc.UI;
<div class="row">
    <div class="col-sm-12">
        <div class="row M-T20">
            <div class="col-sm-11">
                <label style="font-weight:bold;font-size:15px;" class="M-B15">Test Summary</label>
                <table class="table eval-table">
                    <thead>
                        <tr>

                            <th>Section</th>
                            <th>Practical</th>
                            <th>Objective</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>

                            <td>Total Questions</td>
                            <td id="practicalTotalQuestionCount">@Model.Practical.TotalQuestionCount</td>
                            <td id="objectiveTotalQuestionCount">@Model.Objective.TotalQuestionCount</td>
                        </tr>
                        <tr>

                            <td>Right</td>
                            <td id="practicalCorrectAnswersCount">@Model.Practical.CorrectAnswersCount</td>
                            <td id="objectiveCorrectAnswersCount">@Model.Objective.CorrectAnswersCount</td>
                        </tr>
                        <tr>

                            <td>Wrong</td>
                            <td id="practicalIncorrectAnswersCount">@Model.Practical.IncorrectAnswersCount</td>
                            <td id="objectiveIncorrectAnswersCount">@Model.Objective.IncorrectAnswersCount</td>
                        </tr>
                        <tr>

                            <td>Obtained</td>
                            <td id="practicalMarksObtained">@Model.Practical.MarksObtained</td>
                            <td id="objectiveMarksObtained">@Model.Objective.MarksObtained</td>
                        </tr>
                        <tr class="eval-footer">

                            <td>Total</td>
                            <td id="practicalMaximumMarks">@Model.Practical.MaximumMarks</td>
                            <td id="objectiveMaximumMarks">@Model.Objective.MaximumMarks</td>
                        </tr>
                    </tbody>
                </table>
                @*<table class="table table-bordered" style="background:#fff;color:#000;">

                            <thead>
                                <tr>
                                    <th>Section</th>
                                    <th>Total Ques</th>
                                    <th>Correct</th>
                                    <th>Wrong</th>
                                    <th>Obtained</th>
                                    <th>Total</th>
                                </tr>
                           </thead>
                        <tbody>
                            <tr>
                                <td>Practical</td>





                            </tr>
                            <tr>
                                <td>Objective</td>





                            </tr>
                        </tbody>
                    </table>*@
            </div>
        </div>
        <div>
            <div style="font-size:18px;font-weight:600;">Candidate Score  <span id="totalObtainedMarks">@Model.TotalObtainedMarks</span> out of <span id="totalMaximumMarks">@Model.TotalMaximumMarks</span></div>
            <div class="M-T10" style="font-size:15px;font-weight:600;">Has Scored <span id="percentage">@Model.Percentage.ToString("0.00") </span>%</div>
        </div>
        <div>
            <input type="button" value="Submit Evaluation" class="btn btn-lg btn-submitEvaluation" id="btnSubmitEvalution" />
        </div>
    </div>
</div>

